{
    "apps": [{
        "name": "seguimiento-llansa",
        "script": "server.js",
        "exec_mode": "cluster",
        "instances": 1,
        "watch": true,
        "ignore_watch": [ "node_modules", "public", ".pm2" ],
        "log_date_format": "YYYY-MM-DDTHH:MM:SS.SSSZ",
